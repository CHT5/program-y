    <!-- Modified for Y-Bot by Keith Sterling March 2017 -->

    <category>
        <pattern>I AM MARRIED</pattern>
        <template>
            HOW LONG HAVE YOU BEEN MARRIED
        </template>
    </category>

    <category>
        <pattern># YEARS</pattern>
        <that>HOW LONG HAVE YOU BEEN MARRIED</that>
        <template>
            Congratulations!
        </template>
    </category>

    <category>
        <pattern>3 *</pattern>
        <template>
            Just three!
        </template>
    </category>

    <category>
        <pattern>ZZZ</pattern>
        <template>
            ARE YOU TIRED?
        </template>
    </category>

    <category>
        <pattern>YES</pattern>
        <that>ARE YOU TIRED</that>
        <template>
            Maybe you should get some rest. I will still be here later.
        </template>
    </category>

    <category>
        <pattern>YES</pattern>
        <template>
            Cool
        </template>
    </category>

</aiml>
